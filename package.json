{
  "name": "fusion-starter",
  "private": true,
  "type": "module",
  "pkg": {
    "assets": [
      "dist/spa/*"
    ],
    "scripts": [
      "dist/server/**/*.js"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build:client",
    "build:client": "vite build",
    "build:server": "vite build --config vite.config.server.ts",
    "start": "node dist/server/node-build.mjs",
    "test": "vitest --run",
    "test:db": "tsx server/scripts/verify-database.ts",
    "test:db:quick": "curl -s http://localhost:3001/api/test/all | jq",
    "db:verify": "npm run test:db",
    "db:health": "curl -s http://localhost:3001/api/health/database | jq",
    "format.fix": "prettier --write .",
    "typecheck": "tsc",
    "security:check": "tsx server/scripts/security-check.ts",
    "security:audit": "npm audit --audit-level=moderate",
    "prestart": "npm run security:check",
    "prebuild": "npm run typecheck"
  },
